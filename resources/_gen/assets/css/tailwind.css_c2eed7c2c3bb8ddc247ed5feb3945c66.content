/* Global cursor behavior for interactive regions */
a[href],
button,
[role="button"],
[role="menuitem"],
summary,
label[for],
input[type="button"],
input[type="submit"],
input[type="reset"],
select,
.site-kamaboko-corner {
  cursor: pointer;
}

button:disabled,
input:disabled,
select:disabled,
textarea:disabled,
[aria-disabled="true"] {
  cursor: not-allowed;
}

input[type="text"],
input[type="email"],
input[type="search"],
input[type="url"],
input[type="password"],
input[type="number"],
input[type="tel"],
textarea {
  cursor: text;
}

